package de.rwth.ti;

import android.content.BroadcastReceiver;

/**
 * This class gets activated when wifi scan results are available and stores
 * them into database
 * 
 * @author tcuje
 * 
 */
public class ScanReceiver extends BroadcastReceiver {

	private Wavi app;
	private long pid;

	public ScanReceiver(Wavi wifiDemo) {
		super();
		this.app = wifiDemo;
	}

}
